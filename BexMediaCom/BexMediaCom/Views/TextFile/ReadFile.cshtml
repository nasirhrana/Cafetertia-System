    @{
        ViewBag.Title = "Test";
        Layout = "~/Views/Shared/_LayoutCafe.cshtml";
    }
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.10.18/datatables.min.css" />
    <link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.19/css/dataTables.jqueryui.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/1.5.2/css/buttons.jqueryui.min.css" />
    <link href="~/Content/plugins/StyleSheet1.css" rel="stylesheet" />


    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Text File Export Import
                <small>File Export Import</small>
            </h1>
            <ol class="breadcrumb">
                <div class="form-group">
                    <label class="col-sm-3 control-label">Totall: </label>
                    <div class="col-sm-9">
                        <input type="text" readonly="readonly" id="totaluser" style="text-align: center" />
                    </div>
                </div>
            </ol>
        </section>
        <!-----------------------------------------------------------------------------------Common &  Menu Section------------------------------------------------------------------------------->
        <!-- Main content -->
        <section class="content">
  
            <form method="POST" enctype="multipart/form-data">
                <div class="row">
                    <!-- right column -->
                    <div class="col-sm-offset-3 col-md-6">
                        <!-- Horizontal Form -->
                        <div class="box box-info box-form">
                            <!-- form start -->
                            <div class="form-group">
                                <label for="file" class="col-sm-4 control-label" style="text-align: center; padding: 5px">Select File</label>
                                <div class="col-sm-8">
                                    <input type="file" id="file" name="file" />
                                </div>
                            </div>

                        </div><!-- /.box -->
                    </div><!--/.col (right) -->
                </div> <!-- /.row -->

                <div class="row">
                    <!-- right column -->
                    <div class="col-sm-offset-1 col-md-10">
                        <div class="box-size box-danger">
                            <div class="box-header with-border">
                                <input type="button" value="Export" id="Export" class="btn btn-info" />
                                <input type="button" value="Import" id="Import" class="btn btn-danger pull-right" />
                            </div>
                            <!-- /.box-body -->

                        </div><!-- /.box -->
                    </div>
                    <!-- /.col -->
                </div><!-- /.row -->

            </form>
            <!-- Data Table-------------------------------------------------------------------------------------------------------------------------------------------- -->
            <div class="row">
                <div class="col-md-12">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="" style="text-align: center">Employee List</h3>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                            <table id="myTable" class="table table-striped table-bordered table-hover" style="text-align: center; color: green">
                                <thead>
                                    <tr>
                                        <th style="text-align: center">Employee Name</th>
                                        <th style="text-align: center">Proximity</th>
                                        <th style="text-align: center">Division</th>
                                        <th style="text-align: center">Department</th>
                                        <th style="text-align: center">Designation</th>
                                    </tr>
                                </thead>
                                <tbody id="StudentTableBody" style="color: black; align-items: center "></tbody>
                            </table>
                        </div>
                        <!-- /.box-body -->
                    </div>
                    <!-- /.box -->
                </div>
            </div>
        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->
    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
        <script src="~/Content/plugins/InputMask/jquery.inputmask.bundle.min.js"></script>
        <script src="~/JavaScript/JavaScript8.js"></script>
        <script src="~/JavaScript/JavaScript9.js"></script>
        <script src="~/JavaScript/JavaScript10.js"></script>
        <script src="~/JavaScript/JavaScript11.js"></script>
        <script src="~/JavaScript/JavaScript12.js"></script>
        <script src="~/JavaScript/JavaScript1.js"></script>
        <script src="~/JavaScript/JavaScript2.js"></script>
        <script src="~/JavaScript/JavaScript3.js"></script>
        <script src="~/JavaScript/JavaScript4.js"></script>
        <script src="~/JavaScript/JavaScript5.js"></script>

        <script type="text/javascript">
        $(document).ready(function () {
            $('#Export').click(function () {

            var data = new FormData();
            var files = $("#file").get(0).files;
            data.append("Image", files[0]);

            $.ajax({
                url: '@Url.Action("Export", "TextFile")',
                data: data,
                type: 'POST',
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                success: function (data) {
                    $('#myTable').dataTable({
                        destroy: true,
                        dom: 'Bfrtip',
                        buttons: [
                            'excel', 'print'
                        ],
                        data: data,
                        columns: [
                            { 'data': 'Name' },
                            { 'data': 'CardNumber' },
                            { 'data': 'Division' },
                            { 'data': 'Department' },
                            { 'data': 'Designation' }
                          

                        ]
                    });
                 
                }
            });
            });

            $('#Import').click(function () {

                var data = new FormData();
                var files = $("#file").get(0).files;
                data.append("Image", files[0]);

                $.ajax({
                    url: '@Url.Action("Import", "TextFile")',
                    data: data,
                    type: 'POST',
                    contentType: false, // Not to set any content header
                    processData: false, // Not to process data
                    success: function (data) {
                        $('#myTable').dataTable({
                            destroy: true,
                            dom: 'Bfrtip',
                            buttons: [
                                'excel', 'print'
                            ],
                            data: data,
                            columns: [
                                { 'data': 'Name' },
                                { 'data': 'CardNumber' },
                                { 'data': 'Division' },
                                { 'data': 'Department' },
                                { 'data': 'Designation' }


                            ]
                        });

                    }
                });
            });

        });

        </script>
    }

